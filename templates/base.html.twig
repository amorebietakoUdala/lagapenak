<!DOCTYPE html>
<html lang="{{ app.request.locale|split('_')[0] }}">
    <head>
        <meta charset="UTF-8">
        <link rel="icon" href="{{ asset('build/images/favicon.ico') }}">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body>
		{% block header %}
			{{ include('common/header.html.twig') }}
		{% endblock header %}
		{% block menu %}
			{{ include('common/menu.html.twig') }}
		{% endblock %}
		{% block message %}{% endblock message %}
      {% block body %}{% endblock %}
    </body>
</html>
